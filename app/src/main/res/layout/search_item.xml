<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="@drawable/bg">


    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignBaseline="@+id/textView10"
        android:layout_alignBottom="@+id/textView10"
        android:layout_centerHorizontal="true"
        android:text="Housing"
        android:textColor="@android:color/white"
        android:textColorLink="@android:color/white"
        android:textSize="20sp"
        tools:fontFamily="casual" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="Car"
        android:textColor="@android:color/white"
        android:textColorLink="@android:color/white"
        android:textSize="20sp"
        tools:fontFamily="casual"
        android:layout_below="@+id/gridLayout3"
        android:layout_alignLeft="@+id/textView14"
        android:layout_alignStart="@+id/textView14"
        android:layout_marginLeft="27dp"
        android:layout_marginStart="27dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="Furniture"
        android:textColor="@android:color/white"
        android:textColorLink="@android:color/white"
        android:textSize="20sp"
        tools:fontFamily="casual"
        android:layout_below="@+id/gridLayout3"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="18dp"
        android:layout_marginEnd="18dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="Book"
        android:textColor="@android:color/white"
        android:textColorLink="@android:color/white"
        android:textSize="20sp"
        tools:fontFamily="casual"
        android:layout_below="@+id/gridLayout"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="Electronic"
        android:textColor="@android:color/white"
        android:textColorLink="@android:color/white"
        android:textSize="20sp"
        tools:fontFamily="casual"
        android:layout_marginLeft="15dp"
        android:layout_marginStart="15dp"
        android:layout_below="@+id/gridLayout"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="Food"
        android:textColor="@android:color/white"
        android:textColorLink="@android:color/white"
        android:textSize="20sp"
        tools:fontFamily="casual"
        android:layout_marginLeft="13dp"
        android:layout_marginStart="13dp"
        android:layout_below="@+id/gridLayout"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"

        android:layout_marginTop="22dp"
        android:text="QuickBuy"
        android:textColor="@android:color/white"
        android:textColorLink="@android:color/white"
        android:textSize="30sp"
        tools:fontFamily="casual" />

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:clickable="false"
        android:clipChildren="false"
        android:clipToPadding="false"
        android:columnCount="3"
        android:id="@+id/gridLayout3"
        android:layout_marginTop="96dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">


        <ImageButton
            android:id="@+id/car"
            android:layout_width="10dp"
            android:layout_height="76dp"
            android:layout_columnWeight="1"
            android:background="@mipmap/background"
            android:backgroundTint="@color/blueee"
            android:scaleType="fitXY"
            app:srcCompat="@mipmap/car" />

        <ImageButton
            android:id="@+id/house"
            android:layout_width="10dp"
            android:layout_height="76dp"
            android:layout_columnWeight="1"
            android:background="@mipmap/background"
            android:backgroundTint="@color/blueee"
            android:scaleType="fitXY"
            app:srcCompat="@mipmap/house" />

        <ImageButton
            android:id="@+id/furniture"
            android:layout_width="10dp"
            android:layout_height="76dp"
            android:layout_columnWeight="1"
            android:background="@mipmap/background"
            android:backgroundTint="@color/blueee"
            android:scaleType="fitXY"
            app:srcCompat="@mipmap/desk" />

    </GridLayout>

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="72dp"
        android:layout_below="@+id/gridLayout3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/gridLayout">

        <ImageButton
            android:id="@+id/electronic"
            android:layout_width="wrap_content"
            android:layout_height="76dp"
            android:layout_columnWeight="1"
            android:background="@mipmap/background"
            android:backgroundTint="@color/blueee"
            android:scaleType="fitXY"
            app:srcCompat="@mipmap/laptop" />

        <ImageButton
            android:id="@+id/book"
            android:layout_width="wrap_content"
            android:layout_height="76dp"
            android:layout_columnWeight="1"
            android:background="@mipmap/background"
            android:backgroundTint="@color/blueee"
            android:scaleType="fitXY"
            app:srcCompat="@mipmap/library" />

        <ImageButton
            android:id="@+id/food"
            android:layout_width="wrap_content"
            android:layout_height="76dp"
            android:layout_columnWeight="1"
            android:background="@mipmap/background"
            android:backgroundTint="@color/blueee"
            android:scaleType="fitXY"
            app:srcCompat="@mipmap/salad" />
    </GridLayout>

</RelativeLayout>